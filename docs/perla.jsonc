{
  "$schema": "https://raw.githubusercontent.com/AngelMunoz/Perla/main/perla.schema.json",
  "runConfiguration": "development",
  "provider": "jspm",
  "mountDirectories": {
    "/": "./sw",
    "/assets": "./assets",
    "/src": "./src"
  },
  "esbuild": {
    "injects": [
      "./preact-shim.js"
    ],
    "jsxFactory": "h",
    "jsxFragment": "Fragment"
  },
  "build": {
    "includes": [
      "**/**/*.md"
    ]
  },
  "dependencies": [
    {
      "name": "@preact/signals",
      "version": "1.1.2"
    },
    {
      "name": "preact",
      "version": "10.11.3"
    },
    {
      "name": "rxjs",
      "version": "7.5.7"
    },
    {
      "name": "@shoelace-style/shoelace",
      "version": "2.0.0-beta.85"
    },
    {
      "name": "highlight.js",
      "version": "11.6.0"
    },
    {
      "name": "marked",
      "version": "4.1.1"
    },
    {
      "name": "navigo",
      "version": "8.11.1"
    }
  ],
  "devDependencies": []
}